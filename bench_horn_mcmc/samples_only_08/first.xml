<?xml version="1.0" ?>
<VC>
  <FUNDECLS>
    <FUNDECL>
      <ARITY>2</ARITY>
      <NAME>inv</NAME>
    </FUNDECL>
  </FUNDECLS>
  <VARDECLS>
    <VAR>x0</VAR>
    <VAR>x1</VAR>
    <VAR>x2</VAR>
    <VAR>x3</VAR>
    <VAR>x4</VAR>
    <VAR>x5</VAR>
    <VAR>x6</VAR>
    <VAR>x7</VAR>
    <VAR>x8</VAR>
    <VAR>x9</VAR>
  </VARDECLS>
  <ASSERT type="good 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>x3</VAR>
    </VARDECLS>
    <OR>
      <NOT>
        <EQ>
          <VAR>x1</VAR>
          <INT>0</INT>
        </EQ>
      </NOT>
      <NOT>
        <EQ>
          <VAR>x3</VAR>
          <INT>100</INT>
        </EQ>
      </NOT>
    </OR>
  </ASSERT>
  <ASSERT type="0 0">
    <BEFORE>
      <VAR>x1</VAR>
      <VAR>x3</VAR>
    </BEFORE>
    <AFTER>
      <VAR>x2</VAR>
      <VAR>x4</VAR>
    </AFTER>
    <OR>
      <NOT>
        <EQ>
          <VAR>x2</VAR>
          <PLUS>
            <VAR>x1</VAR>
            <INT>1</INT>
          </PLUS>
        </EQ>
      </NOT>
      <AND>
        <OR>
          <AND>
            <OR>
              <NOT>
                <GT>
                  <VAR>x2</VAR>
                  <INT>0</INT>
                </GT>
              </NOT>
              <NOT>
                <GT>
                  <VAR>x3</VAR>
                  <INT>0</INT>
                </GT>
              </NOT>
            </OR>
            <OR>
              <NOT>
                <LE>
                  <VAR>x2</VAR>
                  <INT>0</INT>
                </LE>
              </NOT>
              <NOT>
                <EQ>
                  <VAR>x3</VAR>
                  <INT>5</INT>
                </EQ>
              </NOT>
            </OR>
          </AND>
          <NOT>
            <EQ>
              <VAR>x4</VAR>
              <VAR>x2</VAR>
            </EQ>
          </NOT>
        </OR>
        <OR>
          <AND>
            <GT>
              <VAR>x2</VAR>
              <INT>0</INT>
            </GT>
            <GT>
              <VAR>x3</VAR>
              <INT>0</INT>
            </GT>
          </AND>
          <AND>
            <LE>
              <VAR>x2</VAR>
              <INT>0</INT>
            </LE>
            <EQ>
              <VAR>x3</VAR>
              <INT>5</INT>
            </EQ>
          </AND>
        </OR>
      </AND>
    </OR>
  </ASSERT>
  <ASSERT type="bad 0">
    <VARDECLS>
      <VAR>x1</VAR>
      <VAR>x3</VAR>
    </VARDECLS>
    <OR>
      <GT>
        <VAR>x3</VAR>
        <INT>0</INT>
      </GT>
    </OR>
  </ASSERT>
</VC>

